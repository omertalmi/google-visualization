{
  "manifest_version": 2,

  "name": "Google visualization v2",
  "description": "This extension will hide the knowledge graph",
  "version": "1.0",
    "background": {
    "persistent": false,
    "scripts": ["knowledge.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
"content_scripts": [{
      "matches": ["https://*/*"],
      "js": ["knowledge.js"],
	  "css": ["styles3.css"]
    }],
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "web_accessible_resources": ["css_tree/folder.png"]
  
}