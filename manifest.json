{
  "manifest_version": 2,

  "name": "Google visualization v2",
  "description": "This extension will hide the knowledge graph",
  "version": "1.0",
  "browser_action": {
    "default_title": "That's the tool tip",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
"content_scripts": [{
      "matches": ["https://*/*"],
      "js": ["knowledge.js"],
	  "css": ["styles3.css"]
    }],
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "storage"
  ],
  "web_accessible_resources": ["css_tree/folder.png"]
  
}